generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Component {
  id          Int      @id @default(autoincrement())

  title        String
  description String
  creator     String
  website     String
  github      String

  verified    Boolean @default(false)

  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}