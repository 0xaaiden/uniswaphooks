generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Hook {
  id Int @id @default(autoincrement())

  title       String
  description String
  creator     String
  website     String
  github      String

  category String

  status String @default("pending")

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}